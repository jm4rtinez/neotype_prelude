{
	"name": "@neotype/prelude",
	"version": "0.9.0",
	"description": "Functional programming essentials for TypeScript",
	"repository": {
		"type": "git",
		"url": "https://github.com/jmartinezmaes/neotype_prelude.git"
	},
	"keywords": [
		"adt",
		"algebraic-data-types",
		"do-notation",
		"effects",
		"fluent-api",
		"functional-programming",
		"typescript"
	],
	"author": "Josh Martinez",
	"license": "Apache-2.0",
	"homepage": "https://github.com/jmartinezmaes/neotype_prelude#readme",
	"bugs": {
		"url": "https://github.com/jmartinezmaes/neotype_prelude/issues"
	},
	"files": [
		"dist",
		"src"
	],
	"publishConfig": {
		"access": "public"
	},
	"type": "module",
	"sideEffects": false,
	"engines": {
		"node": "16.* || >= 18.*"
	},
	"exports": {
		"./_*": null,
		"./builder.js": "./dist/builder.js",
		"./cmb.js": "./dist/cmb.js",
		"./cmp.js": "./dist/cmp.js",
		"./either.js": "./dist/either.js",
		"./fn.js": "./dist/fn.js",
		"./ior.js": "./dist/ior.js",
		"./maybe.js": "./dist/maybe.js",
		"./pair.js": "./dist/pair.js",
		"./validation.js": "./dist/validation.js",
		"./package.json": "./package.json"
	},
	"scripts": {
		"clean": "rimraf ./dist",
		"lint": "eslint --ext .ts --max-warnings 0 .",
		"prettier:check": "prettier --check --ignore-path ./.gitignore .",
		"prettier:write": "prettier --write --ignore-path ./.gitignore .",
		"style": "npm run lint && npm run prettier:check",
		"test": "vitest run --coverage",
		"test:watch": "vitest",
		"typecheck": "tsc --skipLibCheck --noEmit",
		"docs": "typedoc",
		"prebuild": "npm run clean",
		"build": "tsc --project ./tsconfig.build.json",
		"prepublishOnly": "npm run build",
		"postpublish": "npm run clean"
	},
	"devDependencies": {
		"@typescript-eslint/eslint-plugin": "^5.55.0",
		"@typescript-eslint/parser": "^5.55.0",
		"@vitest/coverage-v8": "^0.34.1",
		"eslint": "^8.36.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-import": "^2.27.5",
		"fast-check": "^3.7.1",
		"prettier": "2.8.6",
		"prettier-plugin-organize-imports": "^3.2.2",
		"rimraf": "^3.0.2",
		"ts-node": "^10.9.1",
		"typedoc": "0.23.28",
		"typescript": "~5.0",
		"vitest": "^0.34.1"
	},
	"volta": {
		"node": "18.12.1",
		"npm": "9.1.3"
	}
}
